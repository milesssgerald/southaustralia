		<script>
			document.addEventListener('DOMContentLoaded', function () {
				var buildFAQSchema = "";
				buildFAQSchema = "<script type=\"application/ld+json\">{\"@context\":\"https://schema.org\",\"@type\":\"FAQPage\",\"mainEntity\":[";
				let faqElements = $('.accordion .accordion__item');
				if (faqElements.length > 0) {
					faqElements.each(function () {
						let question = $(this).children('.accordion__item__header').text().replace(/[\t\n]+/g, '').replace(/[\"]+/g, "'");
						let answer = $(this).children('.accordion__item__content').text().replace(/[\t]+/g, '').replace(/[\"]+/g, "'");

						// Build FAQ Page Schema
						buildFAQSchema += "{\"@type\":\"Question\",\"name\":\"" + question + "\",\"acceptedAnswer\":{\"@type\":\"Answer\",\"text\":\"" + answer + "\"}},";

					});
					buildFAQSchema = buildFAQSchema.substring(0, buildFAQSchema.length - 1)
					buildFAQSchema += "]}<\/script>";
				}
				// Add FAQ to HTML DOM
				$(buildFAQSchema).appendTo("body");
			}, false);
		</script>

